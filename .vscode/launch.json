		{
			"name": "python file",
			"type": "python",
			"request": "launch",
			"program": "${file}",
			"console": "integratedTerminal"
		},
		{
			"name": "Worker",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/worker.py",
			"console": "integratedTerminal",
			"args": [
				"555", "localhost", "666"
			]
		},
		{
			"name": "Worker 2",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/worker.py",
			"console": "integratedTerminal",
			"args": [
				"222", "localhost", "666"
			]
		},
		{
			"name": "Worker 3",
			"type": "python",
			"request": "launch",
			"program": "${workspaceFolder}/worker.py",
			"console": "integratedTerminal",
			"args": [
				"333", "localhost", "666"
			]
		},
        {
            "name": "Scheduler",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "env": {
                "FLASK_APP": "scheduler.py",
                "FLASK_ENV": "development",
                "FLASK_DEBUG": "0"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload",
                "--host", "0.0.0.0",
                "--port", "666"
            ],
            "jinja": true
        }
	]
}